	keywords = {digital library, TEI, full text search, application, local grammars, Ludwig Wittgenstein, corpus linguistics, editorial philology, XML, applied computational linguistics, OCR, manuscripts, electronic lexicon with semantic annotation, digital humanities, philosophy, lemmatized search},
